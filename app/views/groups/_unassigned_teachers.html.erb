<div  id="teachers-div" class="tab-pane" role="tabpanel1">
  <% @unassigned_leaders.sort_by_last_name.each do | teacher | %>
    <div id="teacher_<%= teacher.id %>" class="teachercard p-2 m-2 bg-info rounded" data-volunteer-id="<%= teacher.id %>" draggable="true">
      <p><strong><%= teacher.full_name %></strong></p>
    </div>
  <% end %>
</div>
